/**
 * Homepage - Helping Hand
 */

.section-homepage--helping_hand {
  padding: 0 0 75px;
  margin-left: auto;
  margin-right: auto;
}
.section-homepage--helping_hand .section-title--hed {
  text-transform: uppercase;
  position: relative;
  margin-bottom: 0;
  padding-bottom: 0;
  font-weight: 600;
}
.section-homepage--helping_hand .section-title--hed span {
  padding: 0;
  margin: -16px 0 0 50px;
  display: block;
  position: relative;
  z-index: -1;
  text-transform: capitalize;
  font-size: 105px;
  line-height: 1;
  font-weight: normal;
}
.section-homepage--helping_hand .section-image {
  position: relative;
  display: inline-block;
  line-height: 0;
  z-index: 10;
  padding-bottom: 40px;
}
.section-homepage--helping_hand .section-image:before,
.section-homepage--helping_hand .section-image:after {
  position: absolute;
  max-width: 360px;
  max-height: 200px;
  width: 60%;
  height: 95%;
  display: block;
  content: "";
  z-index: -1;
  opacity: .75;

  background-image: url('svg-border.svg');
  background-repeat: no-repeat;
  backgrounc-position: center center;
  background-size: contain;

}
.section-homepage--helping_hand .section-image:before {
  top: -15px;
  left: -15px;
}
.section-homepage--helping_hand .section-image:after {
  right: -15px;
  bottom: -15px;
}

@media only screen and (max-width: 767px) {
  .section-homepage--helping_hand .container {
    display: -webkit-box;
    display: -moz-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    flex-flow: column nowrap;
    padding-top: 75px;
  }
  .section-homepage--helping_hand .container div:first-child {
    -webkit-box-ordinal-group: 2;
    -moz-box-ordinal-group: 2;
    -ms-flex-order: 2;
