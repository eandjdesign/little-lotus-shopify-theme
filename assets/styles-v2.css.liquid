    order: 2;
    margin-left: auto;
    margin-right: auto;
    max-width: 300px;
  }
  .section-homepage--helping_hand .container div:last-child {
    -webkit-box-ordinal-group: 1;
    -moz-box-ordinal-group: 1;
    -ms-flex-order: 1;
    -webkit-order: 1;
    order: 1;
    margin: -60px auto 50px auto;
  }
  .section-homepage--helping_hand > div { text-align: center; }
  .section-homepage--helping_hand .section-image:before,
  .section-homepage--helping_hand .section-image:after {
