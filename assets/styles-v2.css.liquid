/**
 * Homepage - Helping Hand
 */

.container.section-homepage--helping_hand {
  padding-top: 75px;
  padding-bottom: 75px;
}

.section-homepage--helping_hand .section-title--hed {
  text-transform: uppercase;
  position: relative;
  margin-bottom: 0;
  padding-bottom: 0;
}

.section-homepage--helping_hand .section-title--hed span {
  padding: 0;
  margin: -16px 0 0 50px;
  display: block;
  position: relative;
  z-index: -1;
  text-transform: capitalize;
  font-size: 105px;
  line-height: 1;
}
.section-homepage--helping_hand .section-image {
  position: relative;
  display: inline-block;
  line-height: 0;
  z-index: 10;
}
.section-homepage--helping_hand .section-image:before,
.section-homepage--helping_hand .section-image:after {
  position: absolute;
  max-width: 360px;
  max-height: 200px;
  width: 60%;
  height: 100%;
  display: block;
  content: "";
  z-index: -1;
  border: 5px solid transparent;
  opacity: .75;

  -webkit-border-image: url('svg-icon--x_border.svg?20161114') 60 round;
  -moz-border-image: url('svg-icon--x_border.svg?20161114') 60 round;
  -ms-border-image: url('svg-icon--x_border.svg?20161114') 60 round;
  -o-border-image: url('svg-icon--x_border.svg?20161114') 60 round;
  border-image: url('svg-icon--x_border.svg?20161114') 60 round;

}
.section-homepage--helping_hand .section-image:before {
  top: -15px;
  left: -15px;
}
.section-homepage--helping_hand .section-image:after {
  right: -15px;
  bottom: -15px;
}

@media only screen and (max-width: 767px) {
  .section-homepage--helping_hand {
    display: flex;
    flex-flow: column nowrap;
  }
  .section-homepage--helping_hand div:first-child { order: 2; }
  .section-homepage--helping_hand div:last-child {
    order: 1;
    margin-bottom: 50px;
    margin-top: -60px;
  }
  .section-homepage--helping_hand > div { text-align: center; }
  .section-homepage--helping_hand .section-image:before,
  .section-homepage--helping_hand .section-image:after {
    -webkit-border-image-slice: 30;
    -moz-border-image-slice: 30;
    -ms-border-image-slice: 30;
    -o-border-image-slice: 30;
    border-image-slice: 30;
  }
  .section-homepage--helping_hand .section-title--hed {
    margin-top: 40px;
    text-align: center;
  }
}

@media only screen and (min-width: 480px) and (max-width: 767px) {
  .section-homepage--helping_hand .section-image {
    max-width: 80%;
    margin-left: auto;
    margin-right: auto;
  }
  .section-homepage--helping_hand .section-image:before,
  .section-homepage--helping_hand .section-image:after {
    max-width: 80%;
    max-height: 80%;
    -webkit-border-image-slice: 40;
    -moz-border-image-slice: 40;
    -ms-border-image-slice: 40;
    -o-border-image-slice: 40;
    border-image-slice: 40;
    border-size: 6px;
  }
  .section-homepage--helping_hand .section-image:before {
    top: -15px;
    left: -15px;
  }
  .section-homepage--helping_hand .section-image:after {
    right: -15px;
    bottom: -15px;
  }
}

@media only screen and (min-width: 768px) and (max-width: 959px) {
  .section-homepage--helping_hand .section-image:before,
  .section-homepage--helping_hand .section-image:after {
    max-width: 360px;
    max-height: 200px;
    -webkit-border-image-slice: 40;
    -moz-border-image-slice: 40;
    -ms-border-image-slice: 40;
    -o-border-image-slice: 40;
    border-image-slice: 40;
  }
  .section-homepage--helping_hand .section-image:before {
    top: -15px;
    left: -15px;
  }
  .section-homepage--helping_hand .section-image:after {
    right: -15px;
    bottom: -15px;
  }
}

@media only screen and (min-width: 960px) and (max-width: 1199px) {
  .section-homepage--helping_hand .section-image:before,
  .section-homepage--helping_hand .section-image:after {
    max-width: 360px;
    max-height: 200px;
    -webkit-border-image-slice: 60;
    -moz-border-image-slice: 60;
    -ms-border-image-slice: 60;
    -o-border-image-slice: 60;
    border-image-slice: 60;
  }
  .section-homepage--helping_hand .section-image:before {
    top: -15px;
    left: -15px;
  }
  .section-homepage--helping_hand .section-image:after {
    right: -15px;
    bottom: -15px;
  }
}




/**
 * Homepage - Testimonials!
 */

.section-homepage--testimonials {
  padding-bottom: 80px;
  text-align: center;
  background-color: rgba(255,153,153,.25) /* #ff9999 */
}

.section-homepage--testimonials .section-title--dek { font-weight: normal; }
.section-homepage--testimonials .testimonials-container {
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  align-items: stretch;
  align-content: stretch;
}
.section-homepage--testimonials .testimonial {
  display: flex;
  flex-flow: column nowrap;
  justify-content: space-between;
}

.section-homepage--testimonials .testimonial-quote {
  background-color: transparent;
  margin: 0 0 30px;
  padding: 0;
  text-align: center;
  font-size: 15px;
  line-height: 1.5;
  font-weight: normal;
}

.section-homepage--testimonials .testimonial-title { font-weight: normal; }
.section-homepage--testimonials .testimonial-title,
.section-homepage--testimonials .testimonial-title span {
  display: block;
}

.section-homepage--testimonials .testimonial-author {
  font-size: 32px;
}

.section-testimonials--ratings {
  display: inline-block;
  position: relative;

}
.section-testimonials--ratings .icon-svg--star_inverted {
  position: absolute;
  right: 0;
  top: 1px;
}


/**
 * Homepage - Products
 */

.section-title--background_flourish {
  position: relative;
  margin-top: 80px;
  margin-bottom: 0;
  text-align: center;
}
.section-title--background_flourish span {
  display: inline-block;
  position: relative;
  padding: 0 15px;
  background-color: #fff;
}
.section-title--background_flourish:before,
.section-title--background_flourish span:before {
  display: block;
  width: 100%;
  height: 100%;
  content: "";
  position: absolute;
  top: 0;
  left: 0;
}
.section-title--background_flourish:before {
  z-index: -1;
  background-image: url('icon--triangles.png');
  background-repeat: repeat-x;
  background-position: left center;
  opacity: .4;
}

.section-homepage--products {
  text-align: center;
}
.section-homepage--products .products-row {
  display: flex;
  flex-flow: row nowrap;
  justify-content: center;
  align-items: stretch;
  align-content: center;
  padding-bottom: 80px;
}
.section-homepage--products .product {
  margin: 0 10px;
  box-sizing: border-box;
  font-weight: normal;
}

@media only screen and (max-width: 767px) {
  .section-homepage--products {
    padding-bottom: 30px;
  }
  .section-homepage--products .section-title--hed {
    margin-top: 30px;
    margin-bottom: 0;
  }
  .section-homepage--products .products-row {
    padding-bottom: 30px;
  }
}

@media only screen and (min-width: 480px) and (max-width: 767px) {
  .section-homepage--products .products-row {
    flex-flow: row wrap;
  }

  .section-homepage--products .product {
    width: 50%;
    margin: 0 0 20px 0;
    padding: 0 20px;
  }
}

@media only screen and (max-width: 480px) {
  .section-homepage--products .section-title--hed:before {
    background-image: none;
  }
  .section-homepage--products .products-row {
    flex-flow: row wrap;
  }
  .section-homepage--products .product {
    width: 100%;
    margin: 0 0 20px 0;
    padding: 0 20px;
  }
}


/**
 * Homepage - Two-up module
 */
.section-homepage--module_two_column {
  margin-bottom: 80px;
}
.section-homepage--module_two_column .section-homepage--module {
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  align-content: center;
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
}
.section-homepage--module_two_column .section-homepage--our_story {
  background-image: url('section-homepage--our_story.jpg');
}
.section-homepage--module_two_column .section-homepage--shop_now {
  background-image: url('section-homepage--shop_now.jpg');
}
.section-homepage--module_two_column .content-box {
  margin: 50px 0 50px 50px;
  padding: 10px;
  min-width: 50%;
  background-color: rgba(255, 255, 255, .95);
  text-align: center;
}
.section-homepage--module_two_column .section-title--hed {
  margin-bottom: 0;
  font-size: 20px;
  font-weight: normal;
}
.section-homepage--module_two_column .section-title--hed span {
  display: block;
  line-height: 1.5;
  margin: 0;
  padding: 0;
  font-size: 40px;
}
.section-homepage--module_two_column .section-title--dek {
  margin-top: -10px;
  text-transform: uppercase;
  border-bottom: 1px solid #8a8c8f;
  display: inline-block;
  color: #8a8c8f;
  font-weight: normal;
}

@media only screen and (min-width: 480px) and (max-width: 767px) {
  .section-homepage--module_two_column .section-homepage--module {
    margin-top: 20px;
    background-position: top center;
  }
}
@media only screen and (max-width: 767px) {
  .section-homepage--module_two_column .container,
  .section-homepage--module_two_column .container .eight.columns { width: 100%; }
  .section-homepage--module_two_column .section-title--hed {
    font-size: 13px;
  }
  .section-homepage--module_two_column .section-title--hed span {
    font-size: 28px;
  }
  .section-homepage--module_two_column .section-title--dek {
    font-size: 11px;
  }
}


